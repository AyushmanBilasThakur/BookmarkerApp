{
    "manifest_version": 2,
    "name": "Bookmarker",
    "description": "Launch your bookmarks easily",
    "version": "0.0.1",
    "icons": {"128": "icon_128.png"},
    "browser_action":
    {
        "default_icon": "icon.png",
        "default_popup": "index.html" 
    },
    "permissions": ["activeTab",
        "system.memory"
    ]


}